{
	"name": "vertretungsapp",
	"version": "0.3.2",
	"author": "Vertretungsapp Development",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:icons:pwa": "pwa-asset-generator ./static/logo/logo.png ./static/ -i ./src/app.html -m ./static/site.webmanifest --background '#111111' --icon-only --path-override '%sveltekit.assets%' && pwa-asset-generator ./assets/splash-dark.png ./static/ -i ./src/app.html -m ./static/site.webmanifest --background '#111111' -p '0px' --splash-only --path-override '%sveltekit.assets%' --darkMode && pwa-asset-generator ./assets/splash.png ./static/ -i ./src/app.html -m ./static/site.webmanifest --background '#EFF1F3' -p '0px' --splash-only --path-override '%sveltekit.assets%' && sed -i 's/%sveltekit.assets%//g' ./static/site.webmanifest",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"release": "release-it",
		"test": ""
	},
	"type": "module",
	"dependencies": {
		"@fontsource/inter": "^5.0.16",
		"@fortawesome/fontawesome-svg-core": "^6.5.1",
		"@fortawesome/free-brands-svg-icons": "^6.5.1",
		"@fortawesome/free-regular-svg-icons": "^6.5.1",
		"@fortawesome/free-solid-svg-icons": "^6.5.1",
		"@types/xml2js": "^0.4.14",
		"@types/xml2json": "^0.11.6",
		"buffer": "^6.0.3",
		"date-fns": "^3.0.6",
		"events": "^3.3.0",
		"stream": "^0.0.2",
		"string_decoder": "^1.3.0",
		"timers": "^0.1.1",
		"tslib": "^2.6.2",
		"xml2js": "^0.6.2"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.0.6",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@typescript-eslint/eslint-plugin": "^6.16.0",
		"@typescript-eslint/parser": "^6.16.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.32",
		"prettier": "^3.1.1",
		"prettier-plugin-organize-imports": "^3.2.4",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.10",
		"pwa-asset-generator": "^6.3.1",
		"svelte": "^4.2.8",
		"svelte-awesome": "^3.3.0",
		"svelte-check": "^3.6.2",
		"svelte-gestures": "^1.5.2",
		"tailwindcss": "^3.4.0",
		"tailwindcss-themer": "^4.0.0",
		"typescript": "^5.3.3",
		"vite": "^5.0.10",
		"vite-plugin-mkcert": "^1.17.1",
		"workbox-routing": "^7.0.0",
		"workbox-strategies": "^7.0.0"
	}
}
